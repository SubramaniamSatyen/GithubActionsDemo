{
    "env": {
        "browser": true,
        "es2021": true
    },
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/react-in-jsx-scope": 0,
        "no-undef": 0
    },
    "settings": {
        "react": {
          "version": "detect"
        }
    },
    "extends": [
        "plugin:react/recommended",
        "standard",
        "plugin:prettier/recommended"
    ],
    "plugins": ["react"]
}
